#!/bin/bash
#
# configuration options for FINAL_NAME
#

RUN_OPTS="-cp lib/*:FINAL_NAME.jar"
JAVA_OPTS="-Djava.system.class.loader=org.structr.StructrClassLoader -Dorg.apache.sshd.registerBouncyCastle=false -server -d64 -Xms1g -Xmx1g -XX:+UseNUMA -XX:+UseConcMarkSweepGC"
MAIN_CLASS="org.structr.Server"
DISPLAY_NAME="FINAL_NAME"
LOG_FILE="/var/log/structr.log"
PID_FILE="/var/run/structr.pid"
